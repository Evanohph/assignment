//structure 
#include <stdio.h>
#include <string.h>
 struct book{
	char title[30];
	char auther[20];
	int Publicationyear;
	char ISBN[13];
	float price; 
	 
}book;
int main(){
    struct book;
	strcpy(book.title, "Introduction to programming");
	book.Publicationyear=2022;
	strcpy(book.ISBN,"9780131103627");
	book.price= 49.99;
	strcpy(book.auther,"John Smith");
	
printf("Title: %s \n",book.title);
printf("Auther %s\n",book.auther);
printf("Publication Year: %d \n",book.Publicationyear);
printf("ISBN: %s \n",book.ISBN);
printf("Price: %.2f \n",book.price);

	
	
	
	
	
	
	
	
	return 0;
}
